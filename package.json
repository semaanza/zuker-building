{
	"name": "zuker-building",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "tinacms dev -c \"astro dev\"",
		"start": "tinacms build && astro dev",
		"build": "tinacms build && pnpm run build:astro",
		"build:astro": "astro check && astro build",
		"preview": "astro preview",
		"astro": "astro"
	},
	"prettier": {
		"semi": false,
		"useTabs": true,
		"tabWidth": 2,
		"singleQuote": false,
		"jsxSingleQuote": false,
		"plugins": [
			"prettier-plugin-tailwindcss",
			"prettier-plugin-astro"
		]
	},
	"dependencies": {
		"@astrojs/check": "^0.4.1",
		"@astrojs/netlify": "^5.1.2",
		"@astrojs/tailwind": "^5.1.0",
		"astro": "^4.2.4",
		"emailjs": "^4.0.3",
		"tailwindcss": "^3.4.1",
		"tinacms": "^2.5.0",
		"typescript": "^5.3.3"
	},
	"devDependencies": {
		"@tinacms/cli": "^1.6.12",
		"@types/node": "^20.11.6",
		"prettier": "3.2.4",
		"prettier-plugin-astro": "^0.13.0",
		"prettier-plugin-tailwindcss": "^0.5.11"
	}
}
